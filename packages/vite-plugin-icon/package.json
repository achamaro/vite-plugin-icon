{
  "name": "@achamaro/vite-plugin-icon",
  "version": "0.0.0",
  "description": "Vite plugin for Iconify Icon",
  "keywords": [
    "vite-plugin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "author": "achamaro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/achamaro/vite-plugin-icon.git",
    "directory": "packages/vite-plugin-icon"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./icon": {
      "types": "./dist/icon.d.ts",
      "import": "./dist/icon.js",
      "require": "./dist/icon.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "devDependencies": {
    "iconify-icon": "^1.0.7",
    "tsup": "^6.7.0"
  },
  "peerDependencies": {
    "vite": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.15.0"
  }
}
